    "pseudocode": [
      "function InsertionSort(array):",
      "  for i from 1 to n-1:",
      "    key = array[i]",
      "    j = i - 1",
      "    while j >= 0 and key < array[j]:",
      "      array[j+1] = array[j]",
      "      j = j - 1",
      "    array[j+1] = key",
      "  return array"
    ]
  },
